local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Null hub lobby", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local Tab = Window:MakeTab({
	Name = "get free unit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "get Engineer Cameraman",
	Callback = function()
local args = {
    [1] = "Engineer Cameraman",
    [2] = 0
}
 
game:GetService("ReplicatedStorage").Game.Events.TakeUnit:FireServer(unpack(args))
 
      		print("button pressed")
  	end    
})
