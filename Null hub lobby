local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Null hub lobby", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "get free unit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "main"
})

Tab:AddButton({
	Name = "get Cinemaman",
	Callback = function()
local args = {
    [1] = "Titan Cinemaman",
    [2] = 0
}
 
game:GetService("ReplicatedStorage").Game.Events.TakeUnit:FireServer(unpack(args))
 
      		print("button pressed")
  	end    
})

Tab:AddButton({
	Name = "get utc",
	Callback = function()
local args = {
    [1] = "Upgraded Titan Cameraman",
    [2] = 0
}
 
game:GetService("ReplicatedStorage").Game.Events.TakeUnit:FireServer(unpack(args))
 
      		print("button pressed")
  	end    
})
